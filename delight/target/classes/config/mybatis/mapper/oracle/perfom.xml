<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mybatis.mapper.perfom">
	
	<select id="selectreserList" parameterType="string" resultType="map">
		select * from detailview
		where mt20id=#{perfomid} and prfdate>=sysdate and prfhour>to_char(sysdate,'HH') and last>0
	</select>
   
</mapper>









